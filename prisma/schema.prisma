// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Coches {
  id Int @id @default(autoincrement())
  nombre String
  precio Int
  info String
  imagen1 String
  imagen2 String?
  imagen3 String?
}
